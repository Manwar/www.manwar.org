  a { color: #555; }
  a:hover { color: white; background-color: white; }
  div.navbar {
    padding: 5px;
    white-space: nowrap;
  }
  div.navbar * {
    margin-left: 10px;
    margin-right: 10px;
    padding: 2px 4px;
  }
  div.navbar a {
    color: #555;
    border-style: ridge;
    text-decoration: none;
    font-weight: bold
  }
  div.navbar a.selected {
    background-color: #eaeaea;
  }
  div.navbar {
    color: #333;
  }
  div.selector {
    font-size: 18pt;
    color: #777;
    white-space: nowrap;
  }
  div.selector a {
    color: #777;
    margin-left: 0.5em;
    margin-right: 0;
  }
  div.selector .selected {
    color: #af3142;
  }
